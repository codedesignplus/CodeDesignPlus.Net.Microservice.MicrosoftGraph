{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000",
        "Protocols": "Http1"
      }
    }
  },
  "Core": {
    "AppName": "ms-microsoftgraph",
    "TypeEntryPoint": "worker",
    "Version": "v1",
    "Description": "Custom Microservice",
    "Business": "CodeDesignPlus",
    "Contact": {
      "Name": "CodeDesignPlus",
      "Email": "support@codedesignplus.com"
    }
  },
  "Security": {
    "IncludeErrorDetails": true,
    "ValidateAudience": true,
    "ValidateIssuer": true,
    "ValidateLifetime": true,
    "RequireHttpsMetadata": true,
    "ValidIssuer": "",
    "ValidAudiences": [
      "a74cb192-598c-4757-95ae-b315793bbbca"
    ],
    "Applications": []
  },
  "Redis": {
    "Instances": {
      "Core": {
        "ConnectionString": "localhost:6379"
      }
    }
  },
  "RedisCache": {
    "Enable": true,
    "Expiration": "00:05:00"
  },
  "RabbitMQ": {
    "Enable": true,
    "Host": "localhost",
    "Port": 5672,
    "UserName": "admin",
    "Password": "password",
    "EnableDiagnostic": true,
    "UseQueue": false
  },
  "Mongo": {
    "Enable": true,
    "ConnectionString": "mongodb://localhost:27017",
    "Database": "db-ms-microsoftgraph",
    "Diagnostic": {
      "Enable": true,
      "EnableCommandText": true
    }
  },
  "Logger": {
    "Enable": true,
    "Level": "Error",
    "OTelEndpoint": "http://127.0.0.1:4317"
  },
  "Observability": {
    "Enable": true,
    "ServerOtel": "http://127.0.0.1:4317",
    "Trace": {
      "Enable": true,
      "AspNetCore": true,
      "GrpcClient": true,
      "SqlClient": false,
      "CodeDesignPlusSdk": true,
      "Redis": true,
      "Kafka": false,
      "RabbitMQ": true
    },
    "Metrics": {
      "Enable": true,
      "AspNetCore": true
    }
  },
  "Vault": {
    "Enable": true,
    "Address": "http://localhost:8200",
    "AppName": "ms-microsoftgraph",
    "Solution": "security-codedesignplus",
    "Token": "root",
    "Mongo": {
      "Enable": true,
      "TemplateConnectionString": "mongodb://{0}:{1}@localhost:27017"
    },
    "RabbitMQ": {
      "Enable": true
    }
  },
  "Graph": {
    "ClientId": "",
    "ClientSecret": "",
    "TenantId": "",
    "Scopes": [
      ""
    ],
    "BaseUrl": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  }
}